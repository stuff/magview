{
  "name": "MagView",
  "description": "abandonware-magazines.org viewer",
  "version": "0.1",
  "manifest_version": 3,
  "icons": {
    "16": "/images/ico16.png",
    "32": "/images/ico32.png",
    "48": "/images/ico48.png",
    "128": "/images/ico128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*.abandonware-magazines.org/*"],
      "css": ["styles.css"],
      "js": ["content-script.js"]
    }
  ]
}
